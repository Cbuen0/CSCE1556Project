
use login;

drop table if exists InvoiceItem;
drop table if exists Item;
drop table if exists Invoice;
drop table if exists Store;
drop table if exists Email;
drop table if exists Person;
drop table if exists Address;

-- -----------------------------------------------------
-- Table Address
-- -----------------------------------------------------
CREATE TABLE IF NOT EXISTS Address (
  addressId INT Primary key NOT NULL AUTO_INCREMENT,
  street VARCHAR(45),
  city VARCHAR(45),
  state VARCHAR(45),
  zip VARCHAR(45),
  country VARCHAR(45)
);


-- -----------------------------------------------------
-- Table Person
-- -----------------------------------------------------
CREATE TABLE IF NOT EXISTS Person (
  personId INT Primary Key NOT NULL AUTO_INCREMENT,
  firstName VARCHAR(45),
  lastName VARCHAR(45),
  personCode VARCHAR(45) NOT NULL,
  addressId INT NOT NULL,
  FOREIGN KEY (addressId) REFERENCES Address (addressId)
);



-- -----------------------------------------------------
-- Table Email
-- -----------------------------------------------------
CREATE TABLE IF NOT EXISTS Email (
  emailId INT Primary Key NOT NULL AUTO_INCREMENT,
  email VARCHAR(255),
  personId INT NOT NULL,
  FOREIGN KEY (personId) REFERENCES Person (personId)
);


-- -----------------------------------------------------
-- Table Store
-- -----------------------------------------------------
CREATE TABLE IF NOT EXISTS Store (
  storeId INT Primary Key NOT NULL AUTO_INCREMENT,
  storeCode VARCHAR(45) NOT NULL,
  manager VARCHAR(45) NOT NULL,
  addressId INT NOT NULL,
  FOREIGN KEY (addressId) REFERENCES Address (addressId)
);


-- -----------------------------------------------------
-- Table Invoice
-- -----------------------------------------------------
CREATE TABLE IF NOT EXISTS Invoice (
  invoiceId INT Primary Key NOT NULL AUTO_INCREMENT,
  invoiceCode VARCHAR(45) NOT NULL,
  customerId INT NOT NULL,
  salespersonId INT NOT NULL,
  storeId INT NOT NULL,
  date VARCHAR(45) NOT NULL,
  FOREIGN KEY (customerId) REFERENCES Person (personId),
  FOREIGN KEY (salespersonId) REFERENCES Person (personId),
  FOREIGN KEY (storeId) REFERENCES Store (storeId)
);


-- -----------------------------------------------------
-- Table Item
-- -----------------------------------------------------
CREATE TABLE IF NOT EXISTS Item (
  itemId INT Primary Key NOT NULL AUTO_INCREMENT,
  name VARCHAR(45) NOT NULL,
  type VARCHAR(45),
  itemCode VARCHAR(45) NOT NULL,
  model VARCHAR(45),
  hourlyRate INT,
  unit VARCHAR(45),
  unitPrice INT
);


-- -----------------------------------------------------
-- Table InvoiceItem
-- -----------------------------------------------------
CREATE TABLE IF NOT EXISTS InvoiceItem (
  invoiceitemId INT Primary Key NOT NULL AUTO_INCREMENT,
  purchasePrice INT,
  leaseRate INT,
  startDate varchar(255),
  endDate varchar(255),
  quantity INT,
  hoursWorked INT,
  itemId INT NOT NULL,
  invoiceId INT NOT NULL,
  FOREIGN KEY (itemId) REFERENCES Item (itemId),
  FOREIGN KEY (invoiceId) REFERENCES Invoice (invoiceId)
   );


